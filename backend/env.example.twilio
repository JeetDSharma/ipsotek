# Twilio SMS Configuration
# Add these variables to your .env file

# Your Twilio Account SID (replace with your actual SID)
TWILIO_ACCOUNT_SID=

# Your Twilio Auth Token (replace with your actual token)
TWILIO_AUTH_TOKEN=

# The phone number you want to receive SMS alerts (with country code)
TWILIO_TO_PHONE=

# Your Twilio phone number (you need to get this from Twilio Console)
# This should be a verified phone number from your Twilio account
TWILIO_FROM_PHONE=

# Enable/disable SMS notifications (true/false)
TWILIO_SMS_ENABLED=true
